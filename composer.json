{
    "name": "soarce/parallel-process-dispatcher",
    "description": "Tiny PHP library for running jobs in background and/or in parallel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Holger Segnitz",
            "email": "holger@segnitz.net"
        }
    ],
    "readme": "README.md",
    "require": {
        "php": ">=7.1,<8.0-DEV"
    },
    "autoload": {
        "psr-4": {
            "FastBill\\ParallelProcessDispatcher\\": "src/"
        }
    },
    "replace": {
      "fastbill/parallel-process-dispatcher": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "jakub-onderka/php-parallel-lint": "*",
        "phploc/phploc": "*",
        "pdepend/pdepend": "*",
        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "*",
        "sebastian/phpcpd": "*",
        "theseer/phpdox": "*"
    }
}
